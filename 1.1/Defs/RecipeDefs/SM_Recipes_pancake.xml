  <RecipeDef>
    <defName>Make_BoxOfPanCake</defName>
    <label>make PanCake Box</label>
    <description>Make a PanCake Box.</description>
    <jobString>Making PanCake Box.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workAmount>2500</workAmount>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RC2_Flour</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>RC2_Sugar</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RC2_Flour</li>
        <li>RC2_Sugar</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <BoxOfPanCake>1</BoxOfPanCake>
    </products>
    <skillRequirements>
      <Cooking>2</Cooking>
    </skillRequirements>
    <workSkill>Cooking</workSkill>
    <researchPrerequisite>Make_HotCake</researchPrerequisite>
  </RecipeDef>
