      <li>ButcherSpot</li>
      <li>TableButcher</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>5.0</count>
      </li>
